{
  "name": "electrode-ota-desktop",
  "productName": "ElectrodeOTA",
  "version": "0.14.0",
  "description": "Electrode OTA (CodePush)",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "hot": "concurrently --kill-others \"npm run eb:dev\"",
    "distro": "npm run eb:release",
    "eb:run": "cross-env NODE_ENV=development  electron --dev app",
    "eb:dev": "npm run eb:webpack-dev-server & cross-env NODE_ENV=development HOT=1 electron --dev app",
    "eb:pack": "npm run eb:compile && build --dir",
    "eb:dist": "cross-env NODE_ENV=production build",
    "eb:release": "cross-env NODE_ENV=production build",
    "eb:webpack-dev-server":"cd ../electrode-ota-ui && cross-env NODE_ENV=development HOT=1 ELECTRODE_ENTRY=../electrode-ota-desktop/app/renderer/index ./node_modules/.bin/webpack-dev-server --hot --inline --config ./webpack.electron.config ",
    "eb:env":"env"
  },
  "build": {
    "appId": "electrode.ota.desktop",
    "dmg": {
      "background": "./background.png",
      "contents": [
        {
          "x": 110,
          "y": 300
        },
        {
          "x": 240,
          "y": 300,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "./electrode-icon.png",
      "target": "dmg"
    },
    "win": {
      "iconUrl": "(windows-only) https link to icon"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gecgithub01.walmart.com/jspear1/electrode-ota-desktop.git"
  },
  "author": {
    "name": "Justin Spears",
    "email": "speajus@gmail.com",
    "url": "https://github.com/jspears"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://gecgithub01.walmart.com/jspear1/electrode-ota-desktop/issues"
  },
  "keywords": [
    "electron",
    "code-push",
    "electrode",
    "react-native",
    "cordova"
  ],
  "homepage": "https://gecgithub01.walmart.com/jspear1/electrode-ota-desktop",
  "devDependencies": {
    "asar": "^0.12.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "boiler-room-custodian": "^0.4.1",
    "chai": "^3.5.0",
    "concurrently": "^3.0.0",
    "cross-env": "^3.0.0",
    "del": "^2.2.2",
    "devtron": "^1.3.0",
    "electron": "^1.4.3",
    "electron-builder": "^7.11.4",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.2.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "fbjs-scripts": "^0.7.1",
    "jsdom": "^9.4.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "node-libs-browser": "^1.0.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-logger": "^2.6.1",
    "sinon": "^1.17.6",
    "spectron": "^3.3.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "code-push-cli": "^1.12.5-beta",
    "css-modules-require-hook": "^4.0.3",
    "electron-debug": "^1.0.1",
    "font-awesome": "^4.6.3",
    "fontawesome": "^4.5.0",
    "plist": "^2.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "semver": "^5.3.0",
    "source-map-support": "^0.4.2",
    "superagent": "^2.3.0",
    "tmp": "0.0.29",
    "which": "^1.2.11",
    "xml2js": "^0.4.17"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  }
}
